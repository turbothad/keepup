{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "public",
  "framework": null,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "handle": "filesystem"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 